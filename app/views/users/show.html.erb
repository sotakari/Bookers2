<div>
  <h3><%= @books.name %></h3>
  <%= book_tag @books.get_profile_image(100,100) %>
</div>

<% @books.each do |book| %>
<div>
  <%= link_to book_path(book.id) do %>
    <%= book.title %>
  <% end %>
  <p><%= image_tag book.user.get_profile_image(100,100) %></p>
  <p>Title：<%= book.title %></p>
  <p>Option：<%= book.body %></p>
</div>
<% end %>